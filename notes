


 encrypt ->
	   blocksize = 6

           iv = aaaaaa
           pt = pad(abcdefghij) = abcdefghij22

	   it1 = pt1 ^ iv = klmnop
	   ct1 = encrypt(it1) => xxxxxx

	   it2 = pt2 ^ ct1 = qrstuv
	   ct2 = encrypt(it2) => yyyyyy
 	   
	   ct = iv + ct1 + ct2
           

oracle ->

		ct = rrrrr0xxxxxxyyyyyy
		iv=rrrrr0
		ct1 = xxxxx
		ct2 = yyyyy

		decrypt(ct1) => it1 = xxxxxx
		pt1 = it1 ^ iv = abcdex

		decrypt(ct2) => it2 = yyyyy
		pt2 = it2 ^ pt1 = ghijxxss2

		pt = unpad(abcdefghij22) = abcdefghij
		
		

ciphertext = xxxxxyyyyy

		ct0 = rrrrr0
		ct1 = xxxxxx
		ct2 = yyyyyy

		
